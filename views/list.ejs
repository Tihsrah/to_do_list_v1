<%- include("header") -%>
    <div class="box" id="heading"> <%= today %> </div>


    <div class="box">
    <!-- <%= test %> -->
      <% for(var i=0; i<test.length ; i++){ %>
       <div class="item">
         <input type="checkbox">
         <p>  <%=test[i]%> </p>
       </div>
      <% } %>

      <form class="item" action="/" method="post">
        <input type="text" name="input" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list" value=<%= today %> >+</button>
      </form>
    </div>
<%- include("footer") -%>
